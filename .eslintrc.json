{
  "extends": [
    "airbnb/base",
    "prettier"
  ],
  "env": {
    "jest": true,
    "node": true
  },
  "plugins": ["prettier"],
  "parser": "babel-eslint",
  "rules": {
    "prettier/prettier": "error",
    "import/prefer-default-export": "off",
    "class-methods-use-this": ["error", { "exceptMethods": ["execute"] }]
  },
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "webpack.config.js"
      }
    }
  }
}
